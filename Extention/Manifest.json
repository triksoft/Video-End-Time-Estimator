{
  "manifest_version": 3,
  "name": "YouTube Video End Time",
  "version": "1.0",
  "description": "Shows what time the current YouTube video will finish playing.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
